{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport movie from './js/movie'; // export default로 내보낼 때 이용\nimport NavBarComponent from './components/Navbar.vue';\nimport ModalComponent from './components/Modal.vue';\nimport EventComponent from './components/Event.vue';\nimport MoviesComponent from './components/Movies.vue';\nimport SearchBarComponent from './components/SearchBar.vue';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      isModal: false,\n      data: movie,\n      data_temp: [...movie],\n      // 영화 데이터 사본\n      selectedMovie: 0,\n      text: \"NETFLIX 강렬한 운명의 드라마, 경성크리처\"\n    };\n  },\n  methods: {\n    handleModalOpen(payload) {\n      this.isModal = true; // 모달창을 열기\n      this.selectedMovie = payload.index; // payload는 객체로 전달됨, key는 인덱스값\n      // payload는 객체로 전달됨, key는 인덱스값  \n      // selectedMovie에 현재 클릭한 영화의 인덱스를 넣어줌\n    },\n    handleIncreaseLike(payload) {\n      this.data[payload.index].like += 1;\n      // payload는 객체로 전달됨, key는 인덱스값  \n    },\n    handleSearchMovie(payload) {\n      let title = payload.movieName;\n      this.data_temp = this.data.filter(movie => {\n        return movie.title.includes(title);\n      });\n    },\n    showAllMovies() {\n      this.data_temp = [...this.data]; // temp 데이터를 원본 데이터로 초기화 할 것\n    }\n  },\n  components: {\n    Navbar: NavBarComponent,\n    Modal: ModalComponent,\n    Event: EventComponent,\n    Movies: MoviesComponent,\n    SearchBar: SearchBarComponent\n  }\n};","map":{"version":3,"names":["movie","NavBarComponent","ModalComponent","EventComponent","MoviesComponent","SearchBarComponent","name","data","isModal","data_temp","selectedMovie","text","methods","handleModalOpen","payload","index","handleIncreaseLike","like","handleSearchMovie","title","movieName","filter","includes","showAllMovies","components","Navbar","Modal","Event","Movies","SearchBar"],"sources":["C:\\Users\\KBDS\\Desktop\\vue3.0\\vue_study\\vue_project_1\\src\\App.vue"],"sourcesContent":["<template>\r\n  <Navbar />\r\n\r\n  <!-- text: Props-->\r\n  <Event :text=\"text\" />\r\n  <SearchBar :data=\"data_temp\" @searchMovie=\"handleSearchMovie\" />\r\n  <p>\r\n    <button @showAllMovies=\"showAllMovies\">전체보기</button>\r\n  </p>\r\n  <!-- $event로 emit의 파라미터를 받음 -->\r\n  <Movies :data=\"data_temp\" @openModal=\"handleModalOpen\" @increaseLike=\"handleIncreaseLike\" />\r\n  <!-- :data (v-bind)로 Modal로 보냄, 마찬가지로 :isModal, :selectedMovie로 해서 Modal로 보냄 (v-bind)-->\r\n  <Modal :data=\"data_temp\" :isModal=\"isModal\" :selectedMovie=\"selectedMovie\" @closeModal=\"isModal = false\" />\r\n</template>\r\n<script>\r\nimport movie from './js/movie';  // export default로 내보낼 때 이용\r\nimport NavBarComponent from './components/Navbar.vue';\r\nimport ModalComponent from './components/Modal.vue';\r\nimport EventComponent from './components/Event.vue';\r\nimport MoviesComponent from './components/Movies.vue';\r\nimport SearchBarComponent from './components/SearchBar.vue';\r\n\r\nexport default {\r\n  name: 'App',\r\n  data() {\r\n    return {\r\n      isModal: false,\r\n      data: movie,\r\n      data_temp: [...movie], // 영화 데이터 사본\r\n      selectedMovie: 0,\r\n      text: \"NETFLIX 강렬한 운명의 드라마, 경성크리처\",\r\n    }\r\n  },\r\n  methods: {\r\n    handleModalOpen(payload) {\r\n      this.isModal = true; // 모달창을 열기\r\n      this.selectedMovie = payload.index; // payload는 객체로 전달됨, key는 인덱스값\r\n      // payload는 객체로 전달됨, key는 인덱스값  \r\n      // selectedMovie에 현재 클릭한 영화의 인덱스를 넣어줌\r\n    },\r\n    handleIncreaseLike(payload) {\r\n      this.data[payload.index].like += 1;\r\n      // payload는 객체로 전달됨, key는 인덱스값  \r\n    },\r\n    handleSearchMovie(payload) {\r\n      let title = payload.movieName;\r\n      this.data_temp = this.data.filter(movie => {\r\n        return movie.title.includes(title);\r\n      })\r\n    },\r\n    showAllMovies() {\r\n      this.data_temp = [...this.data]; // temp 데이터를 원본 데이터로 초기화 할 것\r\n    \r\n    }\r\n  },\r\n  components: {\r\n    Navbar: NavBarComponent,\r\n    Modal: ModalComponent,\r\n    Event: EventComponent,\r\n    Movies: MoviesComponent,\r\n    SearchBar: SearchBarComponent,\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n* {\r\n  box-sizing: border-box;\r\n  margin: 0;\r\n}\r\n\r\nbody {\r\n  max-width: 768px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\nh1,\r\nh2,\r\nh3 {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\np {\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\nbutton {\r\n  margin-right: 10px;\r\n  margin-top: 1rem;\r\n}\r\n\r\n.item {\r\n  width: 100%;\r\n  border: 1px solid #ccc;\r\n  display: flex;\r\n  margin-bottom: 20px;\r\n  padding: 1rem;\r\n}\r\n\r\n.item figure {\r\n  width: 30%;\r\n  margin-right: 1rem;\r\n}\r\n\r\n.item img {\r\n  width: 100%;\r\n}\r\n\r\n.item .info {\r\n  width: 100%;\r\n}\r\n\r\n.modal {\r\n  background: rgba(0, 0, 0, 0.7);\r\n  position: fixed;\r\n  left: 0;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal .inner {\r\n  background: #fff;\r\n  width: 80%;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;AAeA,OAAOA,KAAI,MAAO,YAAY,EAAG;AACjC,OAAOC,eAAc,MAAO,yBAAyB;AACrD,OAAOC,cAAa,MAAO,wBAAwB;AACnD,OAAOC,cAAa,MAAO,wBAAwB;AACnD,OAAOC,eAAc,MAAO,yBAAyB;AACrD,OAAOC,kBAAiB,MAAO,4BAA4B;AAE3D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdD,IAAI,EAAEP,KAAK;MACXS,SAAS,EAAE,CAAC,GAAGT,KAAK,CAAC;MAAE;MACvBU,aAAa,EAAE,CAAC;MAChBC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAACC,OAAO,EAAE;MACvB,IAAI,CAACN,OAAM,GAAI,IAAI,EAAE;MACrB,IAAI,CAACE,aAAY,GAAII,OAAO,CAACC,KAAK,EAAE;MACpC;MACA;IACF,CAAC;IACDC,kBAAkBA,CAACF,OAAO,EAAE;MAC1B,IAAI,CAACP,IAAI,CAACO,OAAO,CAACC,KAAK,CAAC,CAACE,IAAG,IAAK,CAAC;MAClC;IACF,CAAC;IACDC,iBAAiBA,CAACJ,OAAO,EAAE;MACzB,IAAIK,KAAI,GAAIL,OAAO,CAACM,SAAS;MAC7B,IAAI,CAACX,SAAQ,GAAI,IAAI,CAACF,IAAI,CAACc,MAAM,CAACrB,KAAI,IAAK;QACzC,OAAOA,KAA